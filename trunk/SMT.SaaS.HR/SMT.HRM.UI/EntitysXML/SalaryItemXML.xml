    <SALARYITEMCODE></SALARYITEMCODE>
    <SALARYITEMNAME>假期其它扣款</SALARYITEMNAME>
    <SALARYITEMTYPE>2</SALARYITEMTYPE>
    <CALCULATORTYPE>3</CALCULATORTYPE>
    <CALCULATEFORMULA>
      SELECT -(餐费补贴/21.75*( 月度结算.婚假天数 + 月度结算.丧假天数 + 月度结算.路程假天数 + 月度结算.产假天数 + 月度结算.看护假天数 )

      +( CASE   WHEN  年度结算.病假天数  &lt;= 5 THEN ( CASE   WHEN  月度结算.病假天数 &lt;= 1 THEN 0 ELSE (岗位工资+住房补贴+地区差异补贴+餐费补贴)/21.75* (月度结算.病假天数 -1) END ) ELSE (岗位工资+住房补贴+地区差异补贴+餐费补贴)/21.75* 月度结算.病假天数  END ))
      FROM    月度结算  LEFT  JOIN  年度结算  ON  月度结算.EMPLOYEEID  =  年度结算.EMPLOYEEID  AND  月度结算.结算年份  =
      年度结算.结算年份
    </CALCULATEFORMULA>
    <CALCULATEFORMULACODE>
      SELECT  -  ( {86926078-d4c4-4fb6-a9c2-1910843a9309} /  21.75  *  ( T_HR_ATTENDMONTHLYBALANCE.MARRYDAYS  +
      T_HR_ATTENDMONTHLYBALANCE.FUNERALLEAVEDAYS  + T_HR_ATTENDMONTHLYBALANCE.TRIPDAYS  + T_HR_ATTENDMONTHLYBALANCE.MATERNITYLEAVEDAYS  + T_HR_ATTENDMONTHLYBALANCE.NURSESDAYS  )  +  (  CASE   WHEN T_HR_ATTENDYEARLYBALANCE.SICKLEAVEDAYS  &lt;=  5  THEN  (  CASE   WHEN
      T_HR_ATTENDMONTHLYBALANCE.SICKLEAVEDAYS  &lt;=  1  THEN  0  ELSE  ( {fba9f626-ab50-41e6-b479-2649cd7e20d4} +
      {f4ad7117-1d2f-4515-b216-0586d2fd8664} + {58d9c878-150f-4d14-b1a9-d7f07f8527c1} + {86926078-d4c4-4fb6-a9c2-1910843a9309} )
      /  21.75  * (T_HR_ATTENDMONTHLYBALANCE.SICKLEAVEDAYS  -  1) END  )  ELSE  ( {fba9f626-ab50-41e6-b479-2649cd7e20d4} +
      {f4ad7117-1d2f-4515-b216-0586d2fd8664} + {58d9c878-150f-4d14-b1a9-d7f07f8527c1} + {86926078-d4c4-4fb6-a9c2-1910843a9309} )
      /  21.75  * T_HR_ATTENDMONTHLYBALANCE.SICKLEAVEDAYS  END  )  )  FROM  T_HR_ATTENDMONTHLYBALANCE  LEFT  JOIN  T_HR_ATTENDYEARLYBALANCE  ON T_HR_ATTENDMONTHLYBALANCE.EMPLOYEEID  = T_HR_ATTENDYEARLYBALANCE.EMPLOYEEID  AND
      T_HR_ATTENDMONTHLYBALANCE.BALANCEYEAR  = T_HR_ATTENDYEARLYBALANCE.BALANCEYEAR
    </CALCULATEFORMULACODE>
