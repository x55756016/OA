    <CALCULATORTYPE>3</CALCULATORTYPE>
    <CALCULATEFORMULA> SELECT  CASE   WHEN (差额) &lt;= 500 THEN 0.05 WHEN (差额) &lt;= 2000 THEN 0.1 WHEN (差额) &lt;= 5000 THEN 0.15 WHEN (差额) &lt;= 20000 THEN 0.2 WHEN (差额) &lt;= 40000 THEN 0.25 WHEN (差额) &lt;= 60000 THEN 0.3 WHEN (差额) &lt;= 80000 THEN 0.35 WHEN (差额) &lt;= 100000 THEN 0.4 ELSE 0.45 END  FROM  薪资税率  LEFT  JOIN  薪资方案定义  ON  薪资方案定义.SALARYSOLUTIONID  =  薪资税率.SALARYSOLUTIONID  LEFT  JOIN  员工薪资档案  ON  薪资税率.SALARYSOLUTIONID  =  员工薪资档案.SALARYSOLUTIONID </CALCULATEFORMULA>
    <CALCULATEFORMULACODE>SELECT CASE  WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 500 THEN 0.05 WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 2000 THEN 0.1 WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 5000 THEN 0.15 WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 20000 THEN 0.2 WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 40000 THEN 0.25 WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 60000 THEN 0.3 WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 80000 THEN 0.35 WHEN ( {d62310ea-f5ec-4393-862c-da3500722689}) &lt;= 100000 THEN 0.4 ELSE 0.45 END FROM T_HR_SALARYTAXES LEFT JOIN T_HR_SALARYSOLUTION ON T_HR_SALARYSOLUTION.SALARYSOLUTIONID = T_HR_SALARYTAXES.SALARYSOLUTIONID LEFT JOIN T_HR_SALARYARCHIVE ON T_HR_SALARYTAXES.SALARYSOLUTIONID = T_HR_SALARYARCHIVE.SALARYSOLUTIONID </CALCULATEFORMULACODE>
