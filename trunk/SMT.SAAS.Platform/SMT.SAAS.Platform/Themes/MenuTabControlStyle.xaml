                                        Canvas.ZIndex="1"
                                        CornerRadius="3,0,0,3"
                                        IsHitTestVisible="False"
                                        Opacity="0" />
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
